<Window x:Class="QuizzAdventureApp.HighscoreWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizzAdventureApp"
        mc:Ignorable="d"
        Background="Black"
        Title="HighscoreWindow" Height="450" Width="300">
    <StackPanel>
        <ListView x:Name="scoreList" Margin="20">
        </ListView>
        <Button x:Name="menu" Content="Main Menu" Click="menu_Click
                Background="DarkGray" />
    </StackPanel>
</Window>
